{
    "name": "sip",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "sst dev",
        "build": "sst build",
        "build-client": "esbuild src/client/ionic.ts --bundle --minify --outfile=assets/ionic.js",
        "deploy": "sst deploy",
        "remove": "sst remove",
        "console": "sst console",
        "typecheck": "tsc --noEmit",
        "postinstall": "rm -rf ./node_modules/@types/react",
        "db:generate-migrations": "drizzle-kit generate:pg",
        "db:run-migrations": "tsx src/db/run-migrations.ts",
        "db:query": "tsx src/db/query.ts"
    },
    "dependencies": {
        "@neondatabase/serverless": "^0.8.1",
        "dayjs": "^1.11.10",
        "drizzle-orm": "^0.29.3",
        "echarts": "^5.4.3",
        "hono": "^3.10.2",
        "sqlstring": "^2.3.3",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@hono/node-server": "^1.7.0",
        "@tsconfig/node18": "^18.2.2",
        "@types/node": "^20.11.17",
        "@types/uuid": "^9.0.8",
        "aws-cdk-lib": "2.101.1",
        "constructs": "10.2.69",
        "drizzle-kit": "^0.20.14",
        "sst": "2.31.0",
        "ts-node": "^10.9.1",
        "tsx": "^4.7.1",
        "typescript": "^5.2.2"
    },
    "workspaces": [
        "packages/*"
    ]
}
